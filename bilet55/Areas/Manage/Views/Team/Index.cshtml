@using bilet55.Models;

@{
    ViewData["Title"] = "Index";
}
@model ICollection<Team>
<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Id</th>
            <th scope="col">id</th>
            <th scope="col">FullName</th>
            <th scope="col">Position</th>
            <th scope="col">Thought</th>
            <th scope="col">link</th>
            <th scope="col">Delete</th>
            <th scope="col">Update</th>
    </tr>
  </thead>
  <tbody>
        @foreach (var item in Model)
        {
                <tr>

                <td>@item.Id</td>
                <td > @item.ImgUrl.Substring(0,7)</td>
                <td>@item.FullName</td>
                <td>@item.Position</td>
                <td>@item.Thought</td>
                <td>@item.link</td>
                    <td><a  class="btn btn-danger" asp-action="Delete" asp-controller="Team" asp-route-id="@item.Id">Delete</a></td>
                    <td><a class="btn btn-success" asp-action="Update" asp-controller="Team" asp-route-id="@item.Id">Update</a></td>
                </tr>
        }
  </tbody>
</table>

